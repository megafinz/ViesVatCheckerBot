{
  "name": "viesvatchecker",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "func start",
    "start:e2e": "cross-env DOTENV_CONFIG_PATH=./.env.e2e nodemon --watch .env --watch e2e --watch lib --watch HttpAdminApi --watch HttpApi --require dotenv/config --require tsconfig-paths/register e2e/server.ts",
    "test": "cross-env DOTENV_CONFIG_PATH=./.env.test ts-mocha --paths --project ./tsconfig.json --require dotenv/config --timeout 5000 --exit './tests/*.ts'",
    "lint": "npx eslint **/*.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@azure/functions": "^3.5.1",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.4",
    "@types/sinon": "^10.0.16",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.6.0",
    "chai": "^4.3.7",
    "cross-env": "7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "8.10.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "mocha": "^10.0.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "sinon": "^15.2.0",
    "ts-mocha": "10.0.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb-memory-server": "^8.7.0",
    "mongoose": "^6.12.0",
    "soap": "^1.0.0",
    "telegraf": "^4.8.2",
    "zod": "3.21.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
